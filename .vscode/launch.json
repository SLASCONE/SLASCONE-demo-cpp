    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/linux-debug/SLASCONE-demo-cpp/SLASCONE-demo-cpp",
            "args": ["${workspaceFolder}/Assets/License-91fad880-90c4-46cb-8d8b-0a12445c6f0e_scam.xml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/SLASCONE-demo-cpp",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }

    ]
}